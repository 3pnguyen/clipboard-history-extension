{
    "name": "Clipboard History",
    "version": "1.0",
    "description": "History for clipboard.",
    "manifest_version": 3,
    "author": "Ethan Nguyen",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Clipboard History"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}